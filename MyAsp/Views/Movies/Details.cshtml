@model MyAsp.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<p>Data added: @Model.DateAdded.ToShortDateString()</p>
<p>Data release: @Model.DateRelease.ToShortDateString()</p>
<p>Number in stock: @Model.NumberInStock</p>
<p>Genre: @Model.Genre.Name</p>

